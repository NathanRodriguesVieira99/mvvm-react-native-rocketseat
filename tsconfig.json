{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@shared/*": ["./src/shared/*"],
      "@api/*": ["./src/api/*"],
      "@lib/*": ["./src/lib/*"],
      "@services/*": ["./src/domain/services/*"],
      "@presentation/*": ["./src/presentation/*"],
      "@styles/*": ["./src/styles/*"],
      "@assets/*": ["./src/assets/*"],
      "@helpers/*": ["./src/helpers/*"],
      "@screens/*": ["./src/screens/*"]
    }
  }
}
